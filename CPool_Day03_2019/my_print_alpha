void my_putchar(char c)
{
    write( 1, &c, 1);
}
int my_print_alpha(void)
{
    int a;
    
    a = 97;
    while (a <= 122)
    {
        my_putchar(a);
        a = a + 1;
            }
    return(0);
}

int main(void)
{
    my_print_alpha();
    my_putchar('\n');
    return(0);
}

